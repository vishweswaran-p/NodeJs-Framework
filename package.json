{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon index.js",
    "forever": "forever start --uid art-dev -a app",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,./npm-debug.log --copy-files",
    "test": "mocha --recursive ./test/**/*.test.js --compilers js:babel-core/register --timeout 3000",
    "doc": "apidoc -i ./docs -o docs/apidocs",
    "coverage": "nyc --reporter=html --report-dir ./test/code_coverage mocha --recursive ./test/**/*.test.js --compilers js:babel-core/register --timeout 3000"
  },
  "dependencies": {
    "async": "^2.6.1",
    "aws-sdk": "^2.476.0",
    "bcrypt-nodejs": "0.0.3",
    "cookie-parser": "~1.4.3",
    "cron": "^1.5.0",
    "debug": "^4",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-validator": "^5.3.0",
    "fs": "0.0.1-security",
    "libphonenumber-js": "^1.6.9",
    "lodash": "^4.17.10",
    "mandrill-api": "^1.0.45",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.20",
    "morgan": "~1.9.0",
    "mysql": "^2.16.0",
    "mysql-utilities": "^1.0.13",
    "node.js": "0.0.0",
    "nodemailer": "^4.6.8",
    "nyc": "^12.0.2",
    "path": "^0.12.7",
    "pug": "2.0.0-beta11",
    "q": "^1.5.1",
    "random-number": "0.0.9",
    "request": "^2.88.0",
    "sinon": "^6.1.5",
    "uuid": "^3.3.2",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "rimraf": "^2.6.2",
    "yuidocjs": "^0.10.2"
  }
}
